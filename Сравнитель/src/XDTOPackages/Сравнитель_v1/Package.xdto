		<enumeration xsi:type="xs:string">ПроизвольныйАлгоритм</enumeration>
	</valueType>
	<valueType name="НомерБазы" base="xs:int">
		<enumeration xsi:type="xs:string">1</enumeration>
		<enumeration xsi:type="xs:string">2</enumeration>
	</valueType>
	<objectType name="ПравилоСравненияДанных">
		<property name="ПриОпределенииБазы" type="xs:string"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="ПараметрыСравнения" type="d3p1:ПравилоСравненияДанных.ПараметрСравненияДанных" lowerBound="0" upperBound="-1"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="ТаблицыДанных" type="d3p1:ТаблицаДанных" lowerBound="0" upperBound="-1"/>
	</objectType>
	<objectType name="ПравилоСравненияДанных.ПараметрСравненияДанных">
