	</objectType>
	<objectType name="ВыгрузкаДанных.Таблица">
		<property name="Имя" type="xs:NMTOKEN"/>
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="Данные" type="d3p1:ValueStorage"/>
	</objectType>
	<objectType name="ВыгрузкаДанных.ЗначениеПараметра">
		<property name="Имя" type="xs:NMTOKEN"/>
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="Значение" type="d3p1:ValueStorage"/>
	</objectType>
</package>