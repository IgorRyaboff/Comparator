		<property name="ПередВыполнениемЗапроса" type="xs:string"/>
		<property name="ПриВыборкеИзРезультатаЗапроса" type="xs:string"/>
		<property name="ПослеВыполненияЗапроса" type="xs:string"/>
		<property name="ПриВыгрузкеПроизвольнымАлгоритмом" type="xs:string"/>
