		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="Тип" type="d3p1:ОписаниеТипа"/>
	</objectType>
	<objectType name="ОписаниеТипа">
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="БазовыйТип" type="d3p1:БазовыйТипОписанияТипа"/>
		<property name="ДлинаЧисла" type="xs:nonNegativeInteger"/>
		<property name="ТочностьЧисла" type="xs:nonNegativeInteger"/>
		<property name="НеотрицательноеЧисло" type="xs:boolean"/>
		<property name="ДлинаСтроки" type="xs:nonNegativeInteger"/>
		<property name="СтрокаНеограниченнойДлины" type="xs:boolean"/>
		<property name="ДатаСВременем" type="xs:boolean"/>
	</objectType>
	<objectType name="ТаблицаДанных">
