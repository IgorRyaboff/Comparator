	<objectType name="ТаблицаДанных.ПравилоВыгрузки">
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="МеханизмВыгрузки" type="d3p1:МеханизмВыгрузкиДанных"/>
