	</objectType>
	<objectType name="ТаблицаДанных.Поле">
		<property name="Имя" type="xs:NMTOKEN"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="Синоним" type="d3p1:НепустаяСтрока"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="ВидПоля" type="d3p1:ВидПоляТаблицыДанных"/>
