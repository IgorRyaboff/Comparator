<package xmlns="http://v8.1c.ru/8.1/xdto" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" targetNamespace="http://comparator.iryaboff.ru/v1/">
	<import namespace="http://v8.1c.ru/8.1/data/core"/>
	<valueType name="БазовыйТипОписанияТипа" base="xs:string">
		<enumeration xsi:type="xs:string">Число</enumeration>
		<enumeration xsi:type="xs:string">Строка</enumeration>
		<enumeration xsi:type="xs:string">Дата</enumeration>
		<enumeration xsi:type="xs:string">Булево</enumeration>
		<enumeration xsi:type="xs:string">ХранилищеЗначения</enumeration>
		<enumeration xsi:type="xs:string">УникальныйИдентификатор</enumeration>
	</valueType>
	<valueType name="СпособСоединенияВыгруженныхТаблиц" base="xs:string">
		<enumeration xsi:type="xs:string">Левое</enumeration>
		<enumeration xsi:type="xs:string">Правое</enumeration>
		<enumeration xsi:type="xs:string">Внутреннее</enumeration>
		<enumeration xsi:type="xs:string">Полное</enumeration>
	</valueType>
	<valueType name="НепустаяСтрока" base="xs:string" minLength="1"/>
	<valueType name="ВидПоляТаблицыДанных" base="xs:string">
		<enumeration xsi:type="xs:string">Измерение</enumeration>
		<enumeration xsi:type="xs:string">Ресурс</enumeration>
		<enumeration xsi:type="xs:string">Реквизит</enumeration>
	</valueType>
	<valueType name="МеханизмВыгрузкиДанных" base="xs:string">
