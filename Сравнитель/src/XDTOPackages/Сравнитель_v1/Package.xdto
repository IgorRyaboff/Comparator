	<objectType name="ВыгрузкаДанных">
		<property xmlns:d3p1="http://v8.1c.ru/8.1/data/core" name="ИдентификаторВыгрузки" type="d3p1:UUID"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="ПравилоСравнения" type="d3p1:ПравилоСравненияДанных"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="НомерБазы" type="d3p1:НомерБазы"/>
		<property xmlns:d3p1="http://comparator.iryaboff.ru/v1/" name="Таблицы" type="d3p1:ВыгрузкаДанных.Таблица" lowerBound="0" upperBound="-1"/>
